    "lodash": "^4.17.19",
    "nodemailer": "^6.4.6",
    "pg": "^8.0.3",
    "pg-native": "^3.0.0",
    "socket.io": "^2.4.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "minify": "^5.1.1",
